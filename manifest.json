{
	"manifest_version": 2,
	"name": "Henify",
	"description": "Alla heter Hen!",
	"version": "0.1",
	"icons": { "16": "icon_16.png",
		"48": "icon_48.png",
		"128": "icon_128.png"},
	"browser_action": {
		"default_icon": "icon_16.png"
	},
	"background": {
		"page": "background.html"
	},
	"content_scripts":[
	{
		"matches": ["http://*/*", "https://*/*"],
		"js": ["script.js"],
		"run_at": "document_end"
	}
	]

}